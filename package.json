{
  "name": "sw-delta",
  "version": "0.0.0",
  "description": "Incremental download technology for web static assets base on Service Workers",
  "main": "serverside/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test/unit --reporter spec",
    "lint": "jshint ./clientside/*.js ./serverside/*.js ./test/benchmark/*.js ./test/unit/*.js",
    "compile": "browserify -r ./clientside/standalone-sw.js:sw-delta --standalone worker > ./clientside/build/sw-delta.js"
  },
  "keywords": [
    "serviceWorkers",
    "performance",
    "webperf",
    "incremental",
    "delta",
    "versionning"
  ],
  "author": "Gaël Métais",
  "license": "MIT",
  "dependencies": {
    "calmcard": "0.1.1",
    "diff-match-patch": "1.0.0",
    "mime": "1.3.4",
    "q": "1.4.1"
  },
  "devDependencies": {
    "browserify": "13.0.1",
    "chai": "3.2.0",
    "compression": "1.6.2",
    "express": "4.13.4",
    "jshint": "2.9.2",
    "mocha": "2.3.0"
  }
}
